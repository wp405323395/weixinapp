<!--tvcard.wxml-->
<view class="container">
   <view class="item hav">
    <text>智能卡号</text> 
    <text>{{cardInfo.tvCardNumber}}</text>   
  </view>

  <view class="item">
    <text>姓名</text> 
    <text>{{cardInfo.custname}}</text>   
  </view> 
  <view class="item">
    <text>地址</text> 
    <text>{{cardInfo.addr}}</text>   
  </view> 

  <button disabled="{{cardInfo.tvCardNumber == '当前信息为空'}}" bindtap="click" class="nex_btn" hover-class="nex_btn_tap">下一步</button>
  <view bindtap="onMsgConfirm" class="msg_err_wrap" hover-class="err_hove">
    <text class="msg_error">无法匹配卡号信息或信息有误</text>
  </view>
  

</view>

