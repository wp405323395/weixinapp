/* detail.wxss */
.button{
  width: 100%;
  height: 50px;
  background: white;
}
.boomBox{
  position: fixed;
  z-index: 100;
  width: 100%;
  height: 100%;
  background: black;
  top: 0;
  left: 0;
  opacity: 0.5;
}


.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
} 

.details{
  margin-top: 0;
  padding-top: 0;
  overflow: hidden;
  width:70%;
  text-overflow:ellipsis;
  display: -webkit-box;
   -webkit-line-clamp: 2;
   -webkit-box-orient: vertical;
   text-align: right;
   font-size: 0.9rem;
   color: #666666
}
.spacal{
  margin-bottom: 13rpx;
}
.introduce_item{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  box-sizing: border-box;
  align-items: center;
  background: #ffffff;
  padding: 18rpx;
  padding-left: 28rpx;
  padding-right: 28rpx;
  
}
.swiper{
  width: 100%;
  margin: 0;
}


.introduce{
  width: 100%;
}
.lable{
  font-size: 0.9rem;
  color: #333333;
}
.bottom_btn{
  margin-top: 50rpx;
  box-sizing: border-box;
  width: 100%;
}

.introduce>view:last-child{
  margin-top: 13rpx;
}
.bottom_text{
  width: 98%;
  padding-left:25rpx;
  padding-top: 5rpx;
  color: #999999;
  font-size: 23rpx;  
  text-align: left;
}
button{
  background: white;
  border-radius: 0;
  color: #ef2020;
}

.dialog{
  position: fixed;
  width: 90%;
  left: 30%;
  top: 30%;
  z-index: 1000;
    transform: translate(-27.5%,-30%);
}
.dialog1{
  height: 40%;
}
.dialog2{
  height: 70%;
}

.dialog_content{
  background-color: white;
  display: flex;
  flex-direction:column;
  align-items:center ;
  padding-top:16rpx;
  width: 100%;
}
.dialog_t1{
  font-size: 1rem;
  color: white;
  margin-bottom: 2rpx;
}
.dialog_t2{
  font-size: 0.7rem;
  color: white;
}
.dialog_bt{
  font-size: 1rem;
  width: 80%;
  margin-top: 10%;
  border-radius: 10rpx;
}
.dialog_bt1{
  background-color: #3fa5ed;
  color: white;
}
.dialog_bt1_hove{
  background-color: rebeccapurple;
  color: white;
}
.dialog_bt2{
  color: #666666;
  margin-top: 30rpx;
}

.dialog_title_wrap{
  width: 100%;
  height: 100rpx;
  background-color: #ed533f;
  display: flex;
  flex-direction:column;
  align-items:center ;
  justify-content:center;
}
.dialog_title_line{
  display: flex;
  flex-direction: row;
  justify-content:center ;
  align-items:center;

}
.dialog_title_shor_line{
  width: 50rpx;
  height: 1px;
  background-color: white;
  margin: 0 16rpx;
}
.dialog_border{
  display: flex;
  flex-direction: row;
  justify-content:space-between ;
}
.dialog_img{
  width: 30rpx;
  height: 100rpx;
  margin-top: 30rpx;
}
.dialog2_content{
  margin-top: 50rpx;
  width: 100%;
  height: 400rpx;
  background-color: #eeeeee;
  display: flex;
  flex-direction: column;
  justify-content:space-around;
  align-items:center;

}
.tickit_content{
  height: 50%;
  width: 100%;
}
.tickit{
  width: 90%;
  margin-left: 5%;
}
.tickit1{
  margin-top:30rpx;
}
