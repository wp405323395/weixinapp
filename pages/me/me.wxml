<view class="container">
  <image class="me_banner" src="../../img/me_banner.png"/>
  <view class="me_icon">
     <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover" mod></image>
     <text class="userinfo_nickName">{{userInfo.nickName}}</text>
  </view>
  <view class="banner_part"></view>
  <view class="item my_coup" id="my_coup" hover-class="clicked_style" bindtap="onClick">
    <view class="title">
      <image mode="widthFix" src="../../img/my_coup.png" class="title_icon"/>
      <text class="title_text">我的优惠券</text>
    </view>
    <image src="../../img/go_in.png" class="left_btn"></image>
  </view>

  <view class="item my_red_package" id="my_red_package" hover-class="clicked_style" bindtap="onClick">
    <view class="title">
      <image mode="widthFix" src="../../img/my_red_package.png" class="title_icon"/>
      <text class="title_text">我的红包</text>
    </view>
    <image src="../../img/go_in.png" class="left_btn" ></image>
  </view>
  <view class="item my_order" id="my_order" hover-class="clicked_style" bindtap="onClick">
      <view class="title">
      <image mode="widthFix" src="../../img/my_order.png" class="title_icon"/>
      <text class="title_text">我的订单</text>
    </view>
    <image src="../../img/go_in.png" class="left_btn" ></image>
  </view>
  <view class="item my_store" id="my_store" hover-class="clicked_style" bindtap="onClick">
      <view class="title">
      <image mode="widthFix" src="../../img/my_store.png" class="title_icon"/>
      <text class="title_text">我是商家</text>
    </view>
    <image src="../../img/go_in.png" class="left_btn" ></image>
  </view>
</view>
