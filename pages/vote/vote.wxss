/* pages/vote/vote.wxss */
.container{
  background-image: url('https://www.maywidehb.com/banner/vote_content_background.png');
  background-repeat: no-repeat;
  background-size: 100% 100%;
  overflow: scroll;
}
.lable_person{
  margin-left: 30rpx;
  margin-top: 20rpx;
  color: white;
  display: block;
  font-size: 0.7rem;
}
.person_icon{
  opacity: 0.3;
  flex-shrink:0;
  width: 90rpx;
  height: 90rpx;
  margin: 5px;
  margin-bottom: 0rpx;
  border-radius: 45rpx;
  align-items: center;
  justify-content: center;
  display: flex;
  background-color: black;
  color: white;
  font-size: 0.7rem;
}
.personName{
  display: inline-block;
}
.focus{
  font-weight: 600;
  flex-shrink:0;
  width: 110rpx;
  height: 110rpx;
  margin: 15rpx;
  margin-bottom: 0rpx;
  opacity: 0.9;
  border-radius: 55rpx;
  background-color: white;
  align-items: center;
  justify-content: center;
  display: flex;
  color: #7052df;
  font-size: 0.7rem;
}
.person_header{
  height: 200rpx;
  position: fixed;
  z-index: 1000; 
  width: 100%;
  background-image: url('https://www.maywidehb.com/banner/vote_content_background.png');
  background-repeat: no-repeat;
  background-size:  100%;
}
.imgs{
  position: fixed;
  width: 100%;
  overflow-x: scroll;
  white-space: nowrap;
  display: flex;
  align-items: flex-end;
}
/*定义滚动条高宽及背景 高宽分别对应横竖滚动条的尺寸*/  
 ::-webkit-scrollbar  
{  
    width: 0px;  
    height: 0px;  
    background-color: rgba(0, 0, 0, 0);  
}   
.img_page{
  margin-top: 200rpx;
  height: 34rpx;
  margin-bottom: 0;
  margin-left: 30rpx;
  margin-right: 30rpx;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url("data:image/png;base64,");
}
.background_page{
  background-color: white;
  overflow: auto;
  margin-top: -10rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  padding-bottom: 30rpx;
}
.vote_title_background{
  background-image: url('https://www.maywidehb.com/banner/title_background.png');
  background-size:100% 100%;
  background-repeat: no-repeat;
  width: 631rpx;
  height: 150rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx auto;
  margin-bottom: 15rpx;
}
.vote_title{


}
.answer{
  display: flex;
  flex-direction: column;
}
.checkbox_style{
  display: inline-block;
  margin-bottom: 30rpx;
}

.option_text{
  margin-left: 10rpx;
} 
.question_list{
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-bottom: 10rpx;
}
.question{
  padding: 20rpx;
  padding-left: 0;
}
.commons{
  margin: 40rpx;
  margin-top: 20rpx;
  border: 1px #999999 solid;
  padding: 20rpx;
  overflow: hidden;
}
.text_ar{
  width: 100%;
}
.next_person{
  background-color: #7052df;
  margin-bottom: 30rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  color: white;
}
.logo_section{
  display: flex;
}
.logo_img{
  width: 369rpx;
  height: 35rpx;
  margin: 30rpx auto;
}