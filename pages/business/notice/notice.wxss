/* notice.wxss */
.listItem{
  width:100%;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: center;

}

.content{

  width: 100%;
  height: 100%;
  padding: 20rpx auto;
  margin:20rpx 20rpx;
  margin-left: 0rpx;

}
.content .time_wrap{
  margin-bottom: 20rpx;
}

.coup_status{
  width: 50rpx;
  height: 50rpx;
  margin-left: 30rpx;
  margin-right: 20rpx;
  flex-shrink: 0;
  flex-grow: 0;
}
.fl_part{
  margin-left: 20rpx;
  margin-right: 30rpx;
  flex-shrink: 0;
  flex-grow: 0;
}
.coup_name{
  color: 3fa5ed;
}
.unred_img::after{
  content: '';
  display: block;
  width: 15rpx;
  height: 15rpx;
  background-color: red;
  border-radius: 10rpx;
}
.notice_wrap{
  margin: 35rpx 0;
}
.yymmdd{
  margin-left: 25rpx;
  margin-bottom: 15rpx;
}