/* pages/index/index.wxss */

.search{
  background-color: white;
  display: flex;
  flex-direction: row;
  justify-content: center;
  width: 100%;
}
.search_edit{
  background: #eeeeee;
  display: flex;
  width: 550rpx;
  flex-direction: row; 
  border-radius: 50rpx ;
  margin-bottom: 20rpx;
  padding:15rpx 40rpx;
  overflow:hidden;
}
 .search_edit::before{
  font-family: "iconfont";
  content: "\e620";
  font-size: 40rpx;
  color: black;
  font-weight: 500;
  transform: translateX(-20rpx)
}
.search_edit::after{
    font-family: "iconfont";
  content: "\e687";
  font-size: 40rpx;
  color: black;
  font-weight: 500;
 z-index: 1;
  transform: translateX(20rpx);
} 



.tab{float: left; width: 33.3333%;text-align: center;padding: 10rpx 0; }
.topTabSwiper:after{content: ""; clear: both;display: block;}
.tabBorer{border-bottom: 1px solid black; color: black; font-weight: 600}
.swiper{width: 100%;}
.swiper_con{text-align: center; width: 100%; height: 100%; }
.topTabSwiper{
  background: white;
  padding-bottom: 20rpx;
}




.video_style{
  width: 700rpx;
  height: 400rpx;
}
