
.navbar_fix{
  display: flex;  
  background: white;
  margin-bottom: 13rpx; 
  background: green;
  border-bottom: 1rpx solid #D9D9D9;
  position: fixed;
  width: 100%;
  height: 140rpx;
  background: green;
  z-index: 10000;
}
.navbar{  
  width: 100%;
  display: flex;  
  background: white;
  margin-bottom: 13rpx; 
  height: 140rpx;
  border-bottom: 1rpx solid #D9D9D9;
}  
  .item{
  flex: auto;  
  text-align: center;  
  line-height: 80rpx;  
  color: #666666;
  font-size: 0.8rem;
}  
 .item.active{  
  color: #333333;  
  font-size: 0.8rem;
}  
 .item.active:after{  
  content: "";  
  display: block;  
  position: absolute;  
  bottom: 0;  
  left: 0;  
  right: 0;  
  height: 2rpx;  
  background: #ffffff;  
}  
.navbar_item{
  flex: auto;
}
.navbar_img_style{
  width: 50rpx;
  height: 50rpx;
  margin-top: 20rpx;
}
.cell_style{
  display: flex;
  align-items:center;
  flex-direction:column ;
}