
/* page */
page{
background-color:#f9eede;
}
.container {
  background-color: #f9eede;
  height: 100%;
  width: 100%;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.list{
  display: flex;
  flex-direction:row;
  flex-wrap:wrap ;
  justify-content:flex-start ;
}
.list .product_item{
  width: 50%;
  height: 100px;
  display: inline-block;
  
  margin: 20px 0;
}

.list .product_item .product{
background: yellow;
margin: 0px auto;
width: 80px;
height: 80px;
}
.fragment{
  width: 100%;
  height: 100%;
}
.navigator-hover{
  top: 1px; 
  background-color:rgba(0,0,0,0);
  opacity:0.7;
}